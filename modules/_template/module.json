{
  "module_id": "template_module",
  "name": "Template Module",
  "version": "1.0.0",
  "base_url": "http://localhost:9000",
  "health_endpoint": "/health",
  "api_prefix": "/api/v1",
  "features": [
    "example_feature",
    "memory_integration",
    "user_context_aware"
  ],
  "memory_requirements": {
    "read_access": true,
    "write_access": true,
    "search_types": ["hybrid", "neural", "conceptual"],
