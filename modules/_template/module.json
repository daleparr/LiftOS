  ],
  "permissions": [
    "template:read",
    "template:write",
    "memory:read",
    "memory:write"
  ],
  "metadata": {
    "description": "Template module for Lift OS Core integration",
    "author": "Lift Team",
    "category": "template",
    "tags": ["template", "example", "integration"]
  }
}