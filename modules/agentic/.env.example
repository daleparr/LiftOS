# Environment Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
PORT=8000

# LiftOS Service URLs
MEMORY_SERVICE_URL=http://localhost:8001
AUTH_SERVICE_URL=http://localhost:8002
OBSERVABILITY_SERVICE_URL=http://localhost:8003
LLM_SERVICE_URL=http://localhost:8004
CAUSAL_SERVICE_URL=http://localhost:8005

# AI Provider Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
DATABASE_URL=postgresql://agentic:agentic_password@localhost:5433/agentic
REDIS_URL=redis://localhost:6380

# PostgreSQL Configuration
POSTGRES_DB=agentic
POSTGRES_USER=agentic
POSTGRES_PASSWORD=agentic_password

# Evaluation Configuration
DEFAULT_TIMEOUT_SECONDS=300
MAX_CONCURRENT_TESTS=10
EVALUATION_WEIGHTS_FUNCTIONALITY=0.30
EVALUATION_WEIGHTS_RELIABILITY=0.25
EVALUATION_WEIGHTS_PERFORMANCE=0.20
EVALUATION_WEIGHTS_SECURITY=0.15
EVALUATION_WEIGHTS_USABILITY=0.10

# Agent Configuration
MAX_AGENTS_PER_USER=50
DEFAULT_AGENT_TIMEOUT=60
AGENT_SESSION_TTL=3600

# Test Configuration
MAX_TEST_DURATION=1800
TEST_RESULT_RETENTION_DAYS=90
CONCURRENT_TEST_LIMIT=5

# Security Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Monitoring Configuration
ENABLE_METRICS=true
METRICS_PORT=9090
ENABLE_TRACING=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20